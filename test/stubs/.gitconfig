[github]
  user = fcoury
  token = 8f700e0d7747826f3e56ee13651414bd
[color]
  branch = auto
  status = auto
  diff = auto
  ui = auto
[core]
  excludesfile = /home/foo/.gitignore
[branch]
  autosetupmerge = true
[branch "master"]
  rebase = true
  mergeoptions = --ff-only
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol

[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p

